{
  "name": "@tradle/qr-schema",
  "version": "2.0.5",
  "main": "index.js",
  "bin": "./cmd.js",
  "repository": "https://github.com/tradle/qr-schema.git",
  "author": "Mark Vayngrib <mark@tradle.io>",
  "license": "MIT",
  "dependencies": {
    "minimist": "^1.2.0",
    "object.omit": "^3.0.0",
    "protocol-buffers-encodings": "^1.1.1"
  },
  "devDependencies": {
    "protocol-buffers": "^4.2.0",
    "standard": "^16.0.4",
    "tape": "^4.6.3"
  },
  "scripts": {
    "prepare": "protocol-buffers schema.proto -o schema.js && echo \"/* eslint-disable no-var */\\n$(cat schema.js)\\n\" > schema.js && standard --fix",
